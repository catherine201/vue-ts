server {
    listen 80; #配置为前端接口
    server_name localhost

    location / {
        index index.html;
        try_files $uri $uri/ /index.html;
    }
    #static files
     location ~* ^.*\.(jpg|html|jpeg|gif|png|ico|css|zip|tgz|gz|rar|bz2|doc|xls|exe|pdf|ppt|txt|tar|mid|midi|wav|bmp|rtf|js|otf|eot|svg|ttf|woff|woff2)$ {
        expires 30d;
    }
}